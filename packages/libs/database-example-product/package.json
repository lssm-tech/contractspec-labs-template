{
  "name": "@contractspec/lib.database-example-product",
  "version": "0.4.0",
  "scripts": {
    "clean": "rm -rf dist src/generated",
    "lint": "bun run lint:fix",
    "lint:fix": "eslint src --fix",
    "lint:check": "eslint src",
    "publish:pkg": "bun publish --tolerate-republish --ignore-scripts --verbose",
    "publish:pkg:canary": "bun publish:pkg --tag canary",
    "build": "bun run prebuild && bun run build:bundle && bun run build:types",
    "build:bundle": "contractspec-bun-build transpile",
    "build:types": "contractspec-bun-build types",
    "prebuild": "contractspec-bun-build prebuild",
    "typecheck": "tsc --noEmit",
    "dev": "bun run build:bundle --watch",
    "prisma:import": "prisma validate",
    "prisma:check": "prisma validate",
    "db:migrate": "prisma migrate dev",
    "db:deploy:off": "prisma migrate deploy",
    "db:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:push": "prisma db push --skip-generate",
    "db:seed": "bun prisma/seed.ts",
    "db:reset": "prisma db push --skip-generate --force-reset && bun run db:seed",
    "db:reset:full": "bun run db:generate && bun run db:reset",
    "generate": "bun run db:generate"
  },
  "peerDependencies": {
    "@prisma/client": "^7.4.2"
  },
  "dependencies": {
    "@prisma/adapter-pg": "^7.4.2",
    "@prisma/client": "^7.4.2"
  },
  "devDependencies": {
    "@contractspec/app.cli-database": "catalog:contractspec",
    "@contractspec/tool.bun": "catalog:contractspec",
    "dotenv": "catalog:dev",
    "prisma": "^7.4.1"
  },
  "main": "./dist/index.mjs",
  "exports": {
    ".": "./src/index.ts"
  },
  "typesVersions": {
    "*": {
      "*": [
        "src/*"
      ]
    }
  },
  "files": [
    "src",
    "dist",
    "generated"
  ],
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "publishConfig": {
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "bun": "./dist/index.js",
        "node": "./dist/node/index.js",
        "default": "./dist/index.js"
      }
    }
  }
}
